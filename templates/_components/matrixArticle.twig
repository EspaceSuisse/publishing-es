{# Call the matrix article set #}
{% set articleMatrix = entry.fMxArticleBlocks.all() %}

{# loop trough all blocks #}
{% for block in articleMatrix %}

{# switch trough all blocks. actual block code in partials #}
	{% switch block.type %}

		{% case "tSubtitle" %}
			{% include "_partials/cellSubtitle" %}
		{% case "tBox" %}
			{% include "_partials/cellBox" %}
		{% case "tTxtI18n" %}
			{% include "_partials/cellRichText" %}
		{% case "tQuote" %}
			{% include "_partials/cellQuote" %}
		{% case "tArticleImg" %}
			{% include "_partials/cellImg" %}
		{% case "tArticleImgI18n" %}
			{% include "_partials/cellImg" %}

	{% endswitch %}

{% endfor %}